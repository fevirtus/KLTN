<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
<link rel="stylesheet" href="css/account.css"/> 
<link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.4.0/css/font-awesome.min.css" rel="stylesheet" type="text/css"> <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
<script src="js/jquery-1.11.1.min.js"></script>
<body style="background-image: url(https://geniusmindgraphics.com/wp-content/uploads/2017/11/mobile-app-background.jpeg);">
<div class="container" style="margin-right: 180px;"> 
    <div class="row"> 
     <h1 class="text-center" style="color: black;">List Confirm</h1> 
     <div class="col-md-10 col-md-offset-1"> 
      <div class="panel panel-default panel-table" style="width: 1120px"> 
       <div class="panel-heading"> 
        <div class="row"> 
         <div class="col col-xs-6"> 
          <h3 class="panel-title"><form action="vip" method="POST"><input type="submit" value="Search"><input type="text" name="searchValue" ></form></h3>
         </div> 
         <div class="col col-xs-6 text-right"> 
         </div>   
        </div> 
       </div> 
       <div class="panel-body"> 
        <table class="table table-striped table-bordered table-list"> 
         <thead> 
          <tr> 
           <th class="hidden-xs">ID</th> 
           <th>Name</th>
           <th>Confirm Image</th>
           <th>Send At</th>
          </tr> 
         </thead> 
         <tbody>
          <%
        if(vip.length!=0){
        var i=1;
        vip.forEach(function(data){
        %>
        <tr>
          <tr>
            <td class="hidden-xs"><%=data.id %></td>
            <td><input type="text" value="<%= data.name %>" name="name" readonly/></td>
            <td><img style="width: 200px;height: 150px" src="<%=data.confirm_img %>"></td>
            <td><input type="text" value="<%=data.send_time %>" name="send_time" readonly/></td>
           </tr>
        </tr>
        
        <%  i++; }) %>
        <% } else{ %>
            <tr>
                <td colspan="7">No Data Found</td>
            </tr>
        <% } %>
            <% for(var j = 1; j < 10; j++) {%>
              <a href="/vip?page=<%= j %>"><label style="margin: 5px;"><%= j %></label></a>
           <% } %>
        </tbody></table>
       </div>
      </div> 
     </div> 
    </div>
   </div>
  </body>